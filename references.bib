@article{strohmayer_statistical_2013,
  title = {{{ON THE STATISTICAL ANALYSIS OF X-RAY POLARIZATION MEASUREMENTS}}},
  author = {Strohmayer, T. E. and Kallman, T. R.},
  date = {2013-07},
  journaltitle = {The Astrophysical Journal},
  shortjournal = {ApJ},
