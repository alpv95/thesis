@article{zhang_fast_2021,
  title = {Fast Particle Acceleration in Three-Dimensional Relativistic Reconnection},
  author = {Zhang, Hao and Sironi, Lorenzo and Giannios, Dimitrios},
  date = {2021-12-01},
  journaltitle = {The Astrophysical Journal},
  shortjournal = {ApJ},
  volume = {922},
  number = {2},
  eprint = {2105.00009},
  eprinttype = {arxiv},
  primaryclass = {astro-ph},
  pages = {261},
  issn = {0004-637X, 1538-4357},
  doi = {10.3847/1538-4357/ac2e08},
  url = {http://arxiv.org/abs/2105.00009},
  urldate = {2022-10-11},
  abstract = {Magnetic reconnection is invoked as one of the primary mechanisms to produce energetic particles. We employ large-scale three-dimensional (3D) particle-in-cell simulations of reconnection in magnetically-dominated (\$\textbackslash sigma=10\$) pair plasmas to study the energization physics of high-energy particles. We identify a novel acceleration mechanism that only operates in 3D. For weak guide fields, 3D plasmoids / flux ropes extend along the \$z\$ direction of the electric current for a length comparable to their cross-sectional radius. Unlike in 2D simulations, where particles are buried in plasmoids, in 3D we find that a fraction of particles with \$\textbackslash gamma\textbackslash gtrsim 3\textbackslash sigma\$ can escape from plasmoids by moving along \$z\$, and so they can experience the large-scale fields in the upstream region. These "free" particles preferentially move in \$z\$ along Speiser-like orbits sampling both sides of the layer, and are accelerated linearly in time -- their Lorentz factor scales as \$\textbackslash gamma\textbackslash propto t\$, in contrast to \$\textbackslash gamma\textbackslash propto \textbackslash sqrt\{t\}\$ in 2D. The energy gain rate approaches \$\textbackslash sim eE\_\{\textbackslash rm rec\}c\$, where \$E\_\{\textbackslash rm rec\}\textbackslash simeq 0.1 B\_0\$ is the reconnection electric field and \$B\_0\$ the upstream magnetic field. The spectrum of free particles is hard, \$dN\_\{\textbackslash rm free\}/d\textbackslash gamma\textbackslash propto \textbackslash gamma\^\{-1.5\}\$, contains \$\textbackslash sim 20\textbackslash\%\$ of the dissipated magnetic energy independently of domain size, and extends up to a cutoff energy scaling linearly with box size. Our results demonstrate that relativistic reconnection in GRB and AGN jets may be a promising mechanism for generating ultra-high-energy cosmic rays.},
  archiveprefix = {arXiv},
  file = {/Volumes/GoogleDrive/My Drive/Zotero/Zhang et al(2021)Fast particle acceleration in three-dimensional relativistic reconnection.pdf}
}

