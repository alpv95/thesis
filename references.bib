@article{li_electron_2017,
  title = {Electron {{Track Reconstruction}} and {{Improved Modulation}} for {{Photoelectric X-ray Polarimetry}}},
  author = {Li, Tenglin and Zeng, Ming and Feng, Hua and Cang, Jirong and Li, Hong and Zhang, Heng and Zeng, Zhi and Cheng, Jianping and Ma, Hao and Liu, Yinong},
  date = {2017-06},
  journaltitle = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
  shortjournal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
  volume = {858},
  eprint = {1611.07244},
