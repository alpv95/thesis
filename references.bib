@article{danforth_fast_2013,
  title = {A {{FAST FLARE AND DIRECT REDSHIFT CONSTRAINT IN FAR-ULTRAVIOLET SPECTRA OF THE BLAZAR S5}} 0716\$\textbackslash mathplus\$714},
  author = {Danforth, Charles W. and Nalewajko, Krzysztof and France, Kevin and Keeney, Brian A.},
