@article{tchekhovskoy_simulations_2008,
  title = {Simulations of {{Ultrarelativistic Magnetodynamic Jets}} from {{Gamma-ray Burst Engines}}},
  author = {Tchekhovskoy, Alexander and McKinney, Jonathan C. and Narayan, Ramesh},
  date = {2008-08-01},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
  volume = {388},
  number = {2},
  eprint = {0803.3807},
  eprinttype = {arxiv},
  pages = {551--572},
  issn = {00358711, 13652966},
  doi = {10.1111/j.1365-2966.2008.13425.x},
  url = {http://arxiv.org/abs/0803.3807},
  urldate = {2021-01-21},
  abstract = {Long-duration gamma-ray bursts (GRBs) require an engine capable of driving a jet of plasma to ultrarelativistic bulk Lorentz factors of up to several hundred and into narrow opening angles of a few degrees. We use global axisymmetric stationary solutions of magnetically-dominated (force-free) ultrarelativistic jets to test whether the popular magnetic-driving paradigm can generate the required Lorentz factors and opening angles. Our global solutions are obtained via time-dependent relativistic ideal magnetodynamical numerical simulations which follow the jet from the central engine to beyond six orders of magnitude in radius. Our model is primarily motivated by the collapsar model, in which a jet is produced by a spinning black hole or neutron star and then propagates through a massive stellar envelope. We find that the size of the presupernova progenitor star and the radial profile of pressure inside the star determine the terminal Lorentz factor and opening angle of the jet. At the radius where the jet breaks out of the star, our well-motivated fiducial model generates a Lorentz factor \$\textbackslash gamma\textbackslash sim 400\$ and a half-opening angle \$\textbackslash theta\_j\textbackslash sim 2\^\textbackslash circ\$, consistent with observations of many long-duration GRBs. Other models with slightly different parameters give \$\textbackslash gamma\$ in the range 100 to 5000 and \$\textbackslash theta\_j\$ from \$0.1\^\textbackslash circ\$ to \$10\^\textbackslash circ\$, thus reproducing the range of properties inferred for GRB jets. A potentially observable feature of some of our solutions is that the maximum Poynting flux in the jet is found at \$\textbackslash theta \textbackslash sim \textbackslash theta\_j\$ with the jet power concentrated in a hollow cone, while the maximum in the Lorentz factor occurs at an angle \$\textbackslash theta\$ substantially smaller than \$\textbackslash theta\_j\$ also in a hollow cone. [abridged]},
  archiveprefix = {arXiv},
  keywords = {accretion,accretion discs,Astrophysics,black hole physics,galaxies: jets,hydrodynamics,methods: numerical,MHD},
  file = {/Users/ALP/Zotero/storage/62YQK48V/Tchekhovskoy et al. - 2008 - Simulations of Ultrarelativistic Magnetodynamic Je.pdf;/Users/ALP/Zotero/storage/Q7M4TU65/0803.html}
}

